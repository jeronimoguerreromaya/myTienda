    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" prefHeight="46.0" prefWidth="142.0" text="Mi Tienda">
      <font>
         <Font size="31.0" />
      </font></Label>
    <Button onAction="#onRegistrarClienteClick" text="Registrar Cliente" />
   <Button onAction="#onVenderClick" prefHeight="25.0" prefWidth="102.0" text="Vender" />
   <Button onAction="#onVerHistorialClick" prefHeight="25.0" prefWidth="101.0" text="Ver Historial " />
</VBox>
